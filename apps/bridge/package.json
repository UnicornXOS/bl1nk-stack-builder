{
  "name": "bl1nk-agent-bridge",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Edge proxy for bl1nk-agent-builder using Cloudflare Workers",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "wrangler dev",
    "build": "esbuild src/index.ts --bundle --minify --outfile=dist/index.js",
    "deploy": "wrangler deploy",
    "lint": "eslint src --fix",
    "lint:check": "eslint src",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests yet\"",
    "clean": "rm -rf dist",
    "validate": "npm run typecheck && npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "jose": "^4.14.4",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "esbuild": "^0.19.5",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "wrangler": "^3.15.0",
    "@cloudflare/workers-types": "^4.20231016.0"
  },
  "keywords": [
    "cloudflare-workers",
    "edge",
    "proxy",
    "ai",
    "agent",
    "webhook"
  ],
  "author": "bl1nk-agent-builder",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/bl1nk-agent-builder.git"
  }
}